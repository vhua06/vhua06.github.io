<%@ Master Language="c#" Src="page.master.cs" Inherits="MasterPage" %>
<!DOCTYPE html>
<!--[if lt IE 7]>      <html lang="zh-TW" class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html lang="zh-TW" class="no-js lt-ie9 lt-ie8"> <![endif]-->
